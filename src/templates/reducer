import constants from '../constants/@';

const initialState = {};

export default (state = initialState, action) => {
  switch (action.type) {
  case constants.FETCH_*_REQUEST:
  case constants.FETCH_*_SUCCESS:
  case constants.FETCH_*_FAILURE:
  case constants.UPDATE_*:
  case constants.SAVE_*_REQUEST:
  case constants.SAVE_*_SUCCESS:
  case constants.SAVE_*_FAILURE:
    return state;
  default:
    return state;
  }
};
